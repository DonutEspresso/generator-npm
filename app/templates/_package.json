    "chai": "^4.1.2",
<%_ if (data.coveralls) { _%>
    "coveralls": "^3.0.1",
<%_ } _%>
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "prettier": "^1.13.5"
  }
}
