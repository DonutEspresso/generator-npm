{
  "name": "<%= data.name %>",
  "version": "1.0.0",
  "main": "lib/index.js",
  "description": "<%= data.description %>",
  "homepage": "<%= data.homepage %>",
  "author": {
    "name": "<%= data.authorName %>",
    "email": "<%= data.authorEmail %>"
  },
  "repository": {
    "type": "git",
    "url": "<%= data.repoUrl %>"
  },
  "license": "<%= data.license %>",
  "files": [
    "lib"
  ],
  "keywords": [
    "<%= data.name %>"<%
    for (var i = 0; i < data.keywords.length; i++) { %>,
    "<%= data.keywords[i] %>"<%
    } %>
  ],
  "devDependencies": {
    "chai": "^3.5.0",
<%_ if (data.coveralls) { _%>
    "coveralls": "^2.11.14",
<%_ } _%>
    "eslint": "^3.9.1",
    "istanbul": "^0.4.5",
    "jscs": "^2.9.0",
    "mocha": "^3.1.2",
    "nsp": "^2.6.2"
  }
}
