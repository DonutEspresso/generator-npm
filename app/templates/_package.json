  "main": "lib/index.js",
  "description": "<%= data.description %>",
  "homepage": "<%= data.homepage %>",
  "author": {
    "name": "<%= data.authorName %>",
    "email": "<%= data.authorEmail %>"
  },
  "repository": {
    "type": "git",
    "url": "<%= data.repoUrl %>"
  },
  "license": "<%= data.license %>",
  "files": [
    "lib"
  ],
  "keywords": [
